#!/bin/bash

source ~/.zshrc.local
exppath="${LAMP_EXPERIMENTS}/octave"

export MKL_NUM_THREADS=1
export OMP_NUM_THREADS=1

export MATLAB_LOG_DIR=.
export PATH=$PATH:$exppath
export LAMP_octave_results=results/

cd $exppath

# it is not possible to run runner.m directly here because of how importing in Matlab works.
octave main.m
